{
  "name": "aurorapageweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env VITE_SERVICE_ENV=dev  vite",
    "dev-test": "cross-env VITE_SERVICE_ENV=test  vite",
    "build": "vite build",
    "build-prod": "cross-env VITE_SERVICE_ENV=prod vite build",
    "build-ts": "npm run typecheck && cross-env VITE_SERVICE_ENV=dev vite build",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@iconify/vue": "^4.0.2",
    "@vueuse/core": "^9.10.0",
    "axios": "^1.2.2",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "easy-typer-js": "^2.1.0",
    "gsap": "^3.11.4",
    "highlight.js": "^11.7.0",
    "jquery": "^3.6.3",
    "markdown-it": "^13.0.1",
    "medium-zoom": "^1.0.8",
    "pinia": "^2.0.28",
    "qs": "^6.11.0",
    "reading-time-estimator": "^1.7.2",
    "smoothscroll-polyfill": "^0.4.4",
    "splitting": "^1.0.6",
    "swiper": "^8.4.5",
    "vditor": "^3.8.18",
    "vite-plugin-progress": "^0.0.6",
    "vue": "^3.2.45",
    "vue-meta": "3.0.0-alpha.8",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jquery": "^3.5.16",
    "@types/jsdom": "^20.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "jsdom": "^20.0.3",
    "naive-ui": "^2.34.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "unplugin-vue-components": "^0.22.12",
    "vfonts": "^0.0.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
